{
  "expo": {
    "name": "Wordshuffle",
    "slug": "WordShuffle",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#3a71ae",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#3a71ae",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
